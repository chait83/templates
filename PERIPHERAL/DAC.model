	</property>

	<property id="Input" st="O">
		<name>Input</name>
		<defaultValue> 0</defaultValue>
		<veneer type="T">name</veneer>
		<dataType>uint32_t</dataType>
		<type>I</type>
		<desc>Intput of the variable</desc>
	</property>

	<property id="mode">
		<name>MODE</name>
		<veneer type="T">name</veneer>
                <defaultValue> 0</defaultValue>
