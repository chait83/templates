<?xml version="1.0" encoding="UTF-8"?>
<Properties>
	<id type="Array">vector_add</id>
	<name>vect_add</name>
	<size>
		<h>55</h>
		<w>70</w>
	</size>
	<desc>calculate element-by-element addition of two vectors</desc>

		<property id="vector1">
			<name>vect1</name>
			<veneer type="S" />
			<type>I</type>
			<dataType type="Array">uint32_t</dataType>
			<defaultValue>0</defaultValue>
			<desc>set pointer which points to the input vector</desc>
		</property>

		<property id="vector2">
			<name>vect2</name>
			<veneer type="S" />
			<type>I</type>
			<dataType type="Array">uint32_t</dataType>
			<defaultValue>0</defaultValue>
			<desc>set pointer which points to the input vector </desc>
		</property>

		<property id="vector_add_result">
			<name>dot_prod</name>
			<veneer type="S" />
			<type>O</type>
			<dataType type="Array">uint32_t</dataType>
			<defaultValue>0</defaultValue>
			<desc>set pointer for output of vector addition </desc>
		</property>

		<includes>
			<lib>stdio</lib>
			<lib>math</lib>
			<lib>lib</lib>
		</includes>

		<validHost>
			<host>Block</host>
			<host>Loop</host>
		</validHost>

</Properties>
