<?xml version="1.0" encoding="UTF-8"?>
<Properties>
	<id type="Array">Convolution</id>
	<name>Conv</name>
	<size>
		<h>90</h>
		<w>80</w>
	</size>
	<desc> Convolution of input sequences</desc>

	<property id="sequence1">
		<name>sequence1</name>
		<veneer type="S" />
		<type>F</type>
		<dataType type="Array">uint32_t</dataType>
		<defaultValue>sequence1</defaultValue>
		<desc>set pointer which points to the first input sequence</desc>
	</property>

   <property id="seq1Len">
		<name>seq1Len</name>
		<veneer type="S" />
		<dataType>uint32_t</dataType>
		<type>I</type>
		<defaultValue>seq1Len</defaultValue>
		<desc> length of the first input sequence</desc>
	</property>
	
	<property id="sequence2">
		<name>sequence2</name>
		<veneer type="S" />
		<type>F</type>
		<dataType type="Array">uint32_t</dataType>
		<defaultValue>sequence2</defaultValue>
		<desc>set pointer which points to the second input sequence </desc>
	</property>

   <property id="seq2Len">
		<name>seq2Len</name>
		<veneer type="S" />
		<dataType>uint32_t</dataType>
		<type>I</type>
		<defaultValue>seq2Len</defaultValue>
		<desc>length of the second input sequence</desc>
	</property>
	
	<property id="Convolution_result">
		<name>Convolution_result</name>
		<veneer type="S" />
		<type>F</type>
		<dataType type="Array">uint32_t</dataType>
		<defaultValue>Convolution_result</defaultValue>
		<desc>set pointer for output of Convolution  </desc>
	</property>

	<includes>
		<lib>stdio</lib>
		<lib>math</lib>
		<lib>lib</lib>
	</includes>

	<validHost>
		<host>Block</host>
		<host>Loop</host>
	</validHost>

</Properties>
