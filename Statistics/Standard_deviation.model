<?xml version="1.0" encoding="UTF-8"?>
<Properties>
	<id>Standard_deviation</id>
	<name>Std_devi</name>																																																																																																																																																																																																																																																																																																																			</name>
	<size>
		<h>55</h>
		<w>70</w>
	</size>
	<desc> Calculates the Standard_deviation of the input vector</desc>

	<property id="sum_of_square_of_input_vectors">
		<name>sumOfSquares</name>
		<veneer type="S" />
		<type>I</type>
		<dataType type="array">uint32_t</dataType>
		<defaultValue>0</defaultValue>
		<desc>Set pointer to sum of square of input vectors</desc>																
	</property>

<property id="mean_of_input_vector2">
		<name>v_mean</name>
		<veneer type="S" />
		<type>I</type>
		<dataType type="array">uint32_t</dataType>
		<defaultValue>0</defaultValue>
		<desc>Set mean of input vectors</desc>
	</property>
	
	<property id="Blocksize">
		<name>block_size</name>
		<veneer type="S" />
		<dataType>uint32_t</dataType>
		<type>I</type>
		<defaultValue>0</defaultValue>
		<desc>Set Blocksize of input vector</desc>
	</property>

	<property id="output_vector">
		<name>Standard_deviation</name>
		<veneer type="S" />
		<type>I</type>
		<dataType type="array">uint32_t</dataType>
		<defaultValue>0</defaultValue>
		<desc>set output for storing Standard_deviation of input vectors</desc>
	</property>
	<includes>
		<lib>stdio</lib>
		<lib>math</lib>
		<lib>lib</lib>
	</includes>

	<validHost>
		<host>Block</host>
		<host>Loop</host>
	</validHost>

</Properties>
